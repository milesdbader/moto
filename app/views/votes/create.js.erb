(() => {
  const p = document.createElement('p')
  p.innerHTML = "<%= j render 'vote_results', challenge: @challenge %>"

  const options = {
    title: "Good job!",
    content: p,
    icon: "success",
    buttons: [false, "Next"]
  }

  swal(options).then(() => {
    window.location = "<%= @next_url %>"
  })
})()
